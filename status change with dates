BEGIN
    UPDATE AB_PO_RATE_POLICY_MAST
    SET status = CASE
        WHEN SYSDATE < TRUNC(EFFECTIVE_START_DATE) THEN 'N'
        WHEN SYSDATE >= TRUNC(EFFECTIVE_START_DATE) AND SYSDATE < TRUNC(EFFECTIVE_END_DATE) + INTERVAL '1' DAY THEN 'Y'
        WHEN SYSDATE >= TRUNC(EFFECTIVE_END_DATE) + INTERVAL '1' DAY THEN 'N'
    END;
END;
